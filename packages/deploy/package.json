{
  "name": "@daydreamsai/deploy",
  "version": "0.3.20",
  "description": "Deploy Daydreams agents to Google Cloud with custom domains",
  "type": "module",
  "bin": {
    "daydreams-deploy": "./bin/daydreams-deploy"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@google-cloud/cloudbuild": "^5.2.0",
    "@google-cloud/container": "^6.4.0",
    "@google-cloud/dns": "^ 5.3.0",
    "@google-cloud/run": "^3.0.0",
    "@google-cloud/storage": "^7.16.0",
    "chalk": "^5.5.0",
    "commander": "^14.0.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "tar": "^6.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.23",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "@types/tar": "^6.1.10",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "daydreams",
    "deploy",
    "gcp",
    "cloud-run",
    "serverless"
  ],
  "author": "Daydreams AI",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
